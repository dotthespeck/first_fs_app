<h1>Welcome to this site. We haz jobses</h1>

<% @jobs.each do |job| %>
  <h3><a href="<%=job_path(job.id)%>>"<%= job.title %></a></h3>
  <p><%= job.description %></p>
  <h5><%= link_to "Edit Posting", edit_job_path(job)%></h5>
  <h5><%= link_to 'Delete Posting', job, method: :delete %></h5>
<% end %>
